@model CreacionTiposDocumentosViewModel

@{
	ViewData["Title"] = "Editar Tipo Documento";

}

<div class="pagetitle">
    <h1>Editar Documento</h1>
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="TiposDocumentosProvCliente">Home</a></li>
            <li class="breadcrumb-item active">editar documento</li>
        </ol>
    </nav>
</div><!-- End Page Title -->


<a class="btn btn-outline-primary btn-sm mb-4" asp-action="Index" asp-controller="TiposDocumentosProvCliente">Regresar</a>


<div class="col-lg-10">

    <div class="card">
        <div class="card-body">

            <!-- Vertical Form -->
            <form class="row g-3 mt-3" asp-action="Actualizar" asp-controller="TiposDocumentosProvCliente">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <input asp-for="Id" type="hidden"/>

                <div class="col-12">
                    <label asp-for="Descripcion" class="form-label"></label>
                    <input asp-for="Descripcion" type="text" class="form-control">
                    <span asp-validation-for="Descripcion" class="text-danger"></span>
                </div>

                <input asp-for="Es_Activo" type="hidden"/>

                <div class="text-center">
                    <button type="submit" class="btn btn-primary">Guardar</button>
                    <button type="reset" class="btn btn-secondary">Restablecer</button>
                </div>
            </form>

        </div>
    </div>

</div>


@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}




